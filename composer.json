{
  "name": "derakht/jalali",
  "description": "This package is an extension of laravel carbon",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Farhad Shirmardi",
      "email": "shirmardi@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^12.0",
    "illuminate/validation": "^12.0",
    "nesbot/carbon": ">=3.0"
  },
  "autoload": {
    "psr-4": {
      "Derakht\\Jalali\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^3.0",
    "laravel/pint": "^1.21"
  },
  "autoload-dev": {
    "psr-4": {
      "Derakht\\Jalali\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": false
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        ""
      ]
    }
  }
}
