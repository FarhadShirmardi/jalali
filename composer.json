{
  "name": "derakht/jalali",
  "description": "This package is an extension of laravel carbon",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Farhad Shirmardi",
      "email": "shirmardi@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/validation": "^8.0|^9.0|^10.0",
    "nesbot/carbon": ">=2.53"
  },
  "autoload": {
    "psr-4": {
      "Derakht\\Jalali\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.24|^7.5",
    "pestphp/pest": "^1.21",
    "nunomaduro/larastan": "2.0|^1.0.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Derakht\\Jalali\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": false
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        ""
      ]
    }
  }
}
